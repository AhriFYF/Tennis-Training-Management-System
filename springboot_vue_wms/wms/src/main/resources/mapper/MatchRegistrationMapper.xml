<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wms.mapper.MatchRegistrationMapper">

    <resultMap id="BaseResultMap" type="com.wms.entity.match_registration">
        <id column="id" property="id" />
        <result column="student_id" property="studentId" />
        <result column="group_type" property="groupType" />
        <result column="registration_fee" property="registrationFee" />
        <result column="payment_status" property="paymentStatus" />
        <result column="registration_date" property="registrationDate" />
    </resultMap>

    <sql id="Base_Column_List">
        id, student_id, group_type, registration_fee, payment_status, registration_date
    </sql>

    <select id="pageC" resultType="com.wms.entity.match_registration">
        select * from monthly_match_registration
    </select>

    <select id="pageCC" resultType="com.wms.entity.match_registration">
        select * from monthly_match_registration ${ew.customSqlSegment}
    </select>
</mapper>
