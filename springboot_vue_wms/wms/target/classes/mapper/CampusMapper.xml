<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wms.mapper.CampusMapper">

    <resultMap id="BaseResultMap" type="com.wms.entity.Campus">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="address" property="address" />
        <result column="contact_person" property="contactPerson" />
        <result column="contact_phone" property="contactPhone" />
        <result column="contact_email" property="contactEmail" />
        <result column="parent_campus_id" property="parentCampus" />
        <result column="is_center" property="isCenter" />
    </resultMap>

    <sql id="Base_Column_List">
        id, name, address, contact_person, contact_phone, contact_email, parent_campus_id, is_center
    </sql>

    <select id="pageC" resultType="com.wms.entity.Campus">
        select * from campuses
    </select>

    <select id="pageCC" resultType="com.wms.entity.Campus">
        select * from campuses ${ew.customSqlSegment}
    </select>
</mapper>
